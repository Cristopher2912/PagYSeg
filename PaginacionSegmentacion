# Importar la librería necesaria para la paginación
from math import ceil

# Definir una función para obtener los datos (simulado con una lista de números)
def get_data():
    return list(range(1, 21))

# Definir una función para obtener los datos de una página específica
def get_page_data(data_list, page_number, page_size):
    start_index = (page_number - 1) * page_size
    end_index = min(start_index + page_size, len(data_list))
    return data_list[start_index:end_index]

# Definir una función para mostrar los datos de una página
def display_page(data_list, page_number, page_size):
    page_data = get_page_data(data_list, page_number, page_size)
    print("Página", page_number, ":")
    for item in page_data:
        print(item)
    print("-----")

# Obtener los datos
data_list = get_data()

# Definir el tamaño de página
page_size = 5

# Calcular el número total de páginas
total_pages = ceil(len(data_list) / page_size)

# Mostrar cada página de datos
for current_page in range(1, total_pages + 1):
    display_page(data_list, current_page, page_size)
